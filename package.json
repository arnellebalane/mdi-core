{
  "name": "mdi-core",
  "version": "0.0.0-development",
  "description": "Node module for generating Material Design icons",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint *.js"
  },
  "keywords": [
    "mdi",
    "material-design",
    "icons"
  ],
  "files": [
    "index.js",
    "index.html"
  ],
  "author": "Arnelle Balane <arnellebalane@gmail.com> (https://arnellebalane.com/)",
  "license": "MIT",
  "dependencies": {
    "@mdi/svg": "3.0.39",
    "find-node-modules": "1.0.4",
    "handlebars": "4.0.12",
    "puppeteer": "1.10.0"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "commitizen": "3.0.4",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "5.9.0",
    "eslint-config-arnellebalane": "1.7.0",
    "ghooks": "2.0.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "ghooks": {
      "pre-push": "npm run lint"
    }
  },
  "eslintConfig": {
    "extends": [
      "arnellebalane"
    ],
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "env": {
      "node": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arnellebalane/mdi-core.git"
  },
  "bugs": {
    "url": "https://github.com/arnellebalane/mdi-core/issues"
  },
  "homepage": "https://github.com/arnellebalane/mdi-core#readme"
}
